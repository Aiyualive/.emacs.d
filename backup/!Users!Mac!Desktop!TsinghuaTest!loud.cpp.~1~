#include <iostream>#include <string>using namespace std;int main() {  string line;  getline(cin, line);  int n = line[0]-48;  int out[n];  int i = 0;  for (; i < n; i++)  {    string a;    string b;    getline(cin, line);    int k = 0;    for(; line[k] != ' '; k++ ){      a += line[k];    }    //cout << "aoooooo:" << a << endl;    k++;    for(; k < line.length(); k++ ){      b += line[k];    }    //cout << "boooooo:" << b << endl;    int ia = stoi(a);    int ib = stoi(b);    int res = ia + ib;    out[i] = res;  }  i = 0;  int size = (sizeof(out) / sizeof(out[0]));  for (; i < size; i++)  {    cout << out[i] << endl;  }  return 0;}
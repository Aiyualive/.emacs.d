import sys
import random as rnd
rnd_max = (2**32)-1
rnd.seed(0)

#Lecture1-2 slide 26
def ins_srt(a):
    n = len(a)

    for i in range(1,n):
        x = a[i]
        j = i-1
        while (x < a[j]) and (j >= 0):
            a[j+1] = a[j]
            j -= 1
        a[j+1] = x

    return a

n = int(sys.argv[1])
a = [(rnd.randint(0, rnd_max)) for i in range(n)]

shell_srt(a)
